<Page x:Class="_301._3_Development.Pages.AdminPages.RolePages.AdminInfo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_301._3_Development.Pages.AdminPages.RolePages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminInfo">

    <StackPanel Margin="20">

        <TextBlock Text="Access Level"/>
        <ComboBox x:Name="ComboAccessLevel">
            <ComboBoxItem Tag="Low" Content="Low"/>
            <ComboBoxItem Tag="Medium" Content="Medium"/>
            <ComboBoxItem Tag="High" Content="High"/>
        </ComboBox>


    </StackPanel>
</Page>

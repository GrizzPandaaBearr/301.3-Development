<Page x:Class="_301._3_Development.Pages.AdminPages.RolePages.DoctorInfo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_301._3_Development.Pages.AdminPages.RolePages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DoctorInfo">

    <StackPanel Margin="20">
        <TextBlock Text="Specialization"/>
        <TextBox x:Name="TxtSpecialization" />

        <TextBlock Text="Licence Number"/>
        <TextBox x:Name="TxtLicenceNumber"/>

        <TextBlock Text="Office Phone Number"/>
        <TextBox x:Name="TxtPhoneNumber" />
        
        
        
        <TextBlock Text="Experiece (Years)"/>
        <TextBox x:Name="TxtExperience"/>


    </StackPanel>
</Page>
